{
  "name": "kta-desktop",
  "description": "Desktop app KTA PKS.",
  "productName": "KTA PKS",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://github.com/IT-PKS/kta-desktop",
  "main": "dist/main.js",
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "postinstall": "node task-runner.js postinstall",
    "clean-release": "node task-runner.js clean-release",
    "dev": "cross-env NODE_ENV=development PORT=9065 node task-runner.js dev"
  },
  "build": {
    "productName": "KTA PKS",
    "appId": "com.pks.kta",
    "copyright": "Copyright 2020 PKS",
    "directories": {
      "app": "app",
      "buildResources": "resources",
      "output": "release"
    },
    "files": [
      "dist/",
      "!dist/dll/",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "electron": "8.5.0",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.20",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-form": "^8.2.7",
    "babel-loader": "^8.1.0",
    "core-js": "^3.2.1",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "mkdirp": "^1.0.4",
    "react-hot-loader": "^4.12.21",
    "regenerator-runtime": "^0.13.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
